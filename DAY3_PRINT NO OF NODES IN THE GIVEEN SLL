#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node* next;
};

int countNodes(struct Node* head) {
    int count = 0;
    struct Node* current = head;
    
    while (current != NULL) {
        count++;
        current = current->next;
    }
    
    return count;
}

int main() {
    struct Node* head = NULL;
    // Assume the linked list is populated here
    
    int nodesCount = countNodes(head);
    printf("Number of nodes in the linked list: %d\n", nodesCount);
    
    return 0;
}
