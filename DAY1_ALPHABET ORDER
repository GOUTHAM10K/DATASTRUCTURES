// Alphabetical Order
#include <stdio.h>
#include <string.h>

int main() {
    char *arr[] = {"apple", "banana", "orange", "grape", "kiwi"};
    int n = sizeof(arr) / sizeof(arr[0]);
    char *temp;

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (strcmp(arr[i], arr[j]) > 0) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
printf("k.goutham 192311169/n");
    printf("Alphabetical Order: ");
    for (int i = 0; i < n; i++) {
        printf("%s ", arr[i]);
    }

    return 0;
}
