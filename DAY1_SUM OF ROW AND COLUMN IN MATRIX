#include <stdio.h>
int main() {
    int matrix[3][3] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
    int row_sum, col_sum;

    for (int i = 0; i < 3; i++) {
        row_sum = 0;
        col_sum = 0;
        for (int j = 0; j < 3; j++) {
            row_sum += matrix[i][j];
            col_sum += matrix[j][i];
            printf("%d\t", matrix[i][j]);
        }
        printf("Row Sum: %d\n", row_sum);
    }
    printf("Column Sums: %d\t%d\t%d\n", col_sum, col_sum, col_sum);

    return 0;
}
