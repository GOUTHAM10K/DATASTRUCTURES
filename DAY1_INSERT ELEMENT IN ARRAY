#include <stdio.h>
int main() {
    int array[10] = {1, 2, 3, 4, 5, 6, 7, 8, 9};
    int position = 5;
    int newValue = 100;
    int n = 9;
    for (int i = n; i >= position; i--) {
        array[i + 1] = array[i];
    }

    array[position] = newValue;
    n++;

    printf("Array after insertion:\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", array[i]);
    }

    return 0;
}
