#include <stdio.h>
int main() {
    int arr[] = {10, 20, 30, 40, 50};
    int search_element = 30;
    int size = sizeof(arr) / sizeof(arr[0]);
    int i, found = 0;

    for (i = 0; i < size; i++) {
        if (arr[i] == search_element) {
            found = 1;
            break;
        }
    }
    if (found == 1) {
        printf("Element %d found at index %d in the array.", search_element, i);
    } else {
        printf("Element not found in the array.");
    }

    return 0;
}
