#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node* next;
};

void insertAtMiddle(struct Node** head_ref, int new_data) {
    struct Node* new_node = (struct Node*)malloc(sizeof(struct Node));
    struct Node* slow_ptr = *head_ref;
    struct Node* fast_ptr = (*head_ref)->next;

    while (fast_ptr != NULL && fast_ptr->next != NULL) {
        slow_ptr = slow_ptr->next;
        fast_ptr = fast_ptr->next->next;
    }

    new_node->data = new_data;
    new_node->next = slow_ptr->next;
    slow_ptr->next = new_node;
}

void printList(struct Node* node) {
    while (node != NULL) {
        printf("%d ", node->data);
        node = node->next;
    }
}

int main() {
    struct Node* head = NULL;

    insertAtMiddle(&head, 1);
    insertAtMiddle(&head, 2);
    insertAtMiddle(&head, 3);
    insertAtMiddle(&head, 4);

    printf("Linked list: ");
    printList(head);

    return 0;
}
