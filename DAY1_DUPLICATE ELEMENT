#include <stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int new_size = 2 * n;
    int new_arr[new_size];

    for (int i = 0; i < n; i++) {
        new_arr[i] = arr[i];
        new_arr[i + n] = arr[i];
    }

    printf("Original Array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    printf("\nDuplicated Array: ");
    for (int i = 0; i < new_size; i++) {
        printf("%d ", new_arr[i]);
    }

    return 0;
}
